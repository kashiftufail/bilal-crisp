<h1>Add An Area Code</h1>

       
<table border="0" cellpadding="0" cellspacing="0"  id="id-form">
  <%= form_for :area_code, :url => area_codes_path do |f| -%>

    <tr>
      <th valign="top">Post Code *</th>
      <td><%= f.text_field :area_code, :class => 'inp-form' %></td>
    </tr>

    <tr>
      <th valign="top">Area Name *</th>
      <td><%= f.text_field :area_name, :class => 'inp-form' %></td>
    </tr>

    <tr>
		<th>&nbsp;</th>
		<td valign="top">
			<input id="next_step" type="image" src="/images/common/confirm.jpg" />
		</td>
		<td></td>
	</tr>

  <% end -%>
</table>
